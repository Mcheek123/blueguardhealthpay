{
  "version": 2,
  "builds": [
    {
      "src": "templates/home.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/templates/home.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
